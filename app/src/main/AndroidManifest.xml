    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/iconmm"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/iconmm"
        android:supportsRtl="true"
        android:theme="@style/Theme.MamaFood2">
        <activity android:name=".Activity.SignNow"></activity>
        <activity android:name=".Activity.Pay" />
        <activity android:name=".Test.Order" />
        <activity android:name=".Activity.Index" />
        <activity android:name=".Activity.Welcome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeTab.Food" />
        <activity android:name=".HomeTab.Dessert" />
        <activity android:name=".HomeTab.Drink" />
        <activity android:name=".Menu.AccountFrag" />
        <activity android:name=".Menu.NotifiFrag" />
        <activity android:name=".Menu.OrderFrag" />
        <activity android:name=".Menu.HomeFrag" />
        <activity android:name=".Activity.CartActivity" />
        <activity android:name=".Activity.Login" />
        <activity android:name=".Activity.MainActivity" />
    </application>

</manifest>